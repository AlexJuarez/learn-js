{
  "plugins": ["airbnb-base"],
  "parser": "eslint-babel",
  "compilerOptions": {
    "target": "esnext",
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "module": "commonjs",
    "paths": {
      ":data-structures/*": ["data-structures/*"],
      ":algorithms/*": ["algorithms/*"],
      ":solutions/*": ["solutions/*"]
    }
  },
  "include": [ ".ts", ".js" ],
  "exclude": [
    "node_modules"
  ]
}
